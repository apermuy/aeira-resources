<?php

/**
 * @file
 * Installation file for aeirearesources module.
 */

/**
 * Implements hook_install().
 */
function aeiraresources_install() {

    $config = \Drupal::service('config.factory')->getEditable('aeiraresources.settings');

    $config->set('aeira_lat', '43.4421')->save();
    $config->set('aeira_lon', '-8.2634')->save();
    $config->set('aeira_zoom', '14')->save();
}
